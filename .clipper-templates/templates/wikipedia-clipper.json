{
  "schemaVersion": "0.1.0",
  "name": "Wikipedia",
  "behavior": "create",
  "noteContentFormat": "{{selectorHtml:#mw-content-text|remove_html:(\".navbox,.printfooter,.side-box\")|markdown}}",
  "properties": [
    {
      "name": "categories",
      "value": "",
      "type": "multitext"
    },
    {
      "name": "url",
      "value": "{{url}}",
      "type": "text"
    },
    {
      "name": "author",
      "value": "[[Wikipedia]]",
      "type": "multitext"
    },
    {
      "name": "description",
      "value": "",
      "type": "text"
    },
    {
      "name": "topics",
      "value": "",
      "type": "multitext"
    },
    {
      "name": "tags",
      "value": "wikipedia, articles",
      "type": "multitext"
    }
  ],
  "triggers": [
    "/^https:\\/\\/[a-z-.]+\\.wikipedia\\.org\\/wiki\\/.*$/"
  ],
  "noteNameFormat": "{{title}}",
  "path": "Clippings"
}
