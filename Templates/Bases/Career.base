filters:
  and:
    - note.categories.contains(link("Career"))
    - '!file.name.contains("Template")'
    - '!file.inFolder("Categories")'
properties:
  file.name:
    displayName: Name
  note.type:
    displayName: Type
  file.ctime:
    displayName: Added
views:
  - type: table
    name: All
    order:
      - file.name
      - type
      - file.ctime
    sort:
      - property: file.name
        direction: ASC
  - type: table
    name: Type
    filters:
      and:
        - list(type).contains(this)
    order:
      - file.name
      - type
      - file.ctime
    sort:
      - property: file.name
        direction: ASC
