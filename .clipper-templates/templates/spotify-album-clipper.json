{
  "schemaVersion": "0.1.0",
  "name": "Spotify Album",
  "behavior": "create",
  "noteContentFormat": "{{selector:[data-testid=\"internal-track-link\"]|wikilink|join:\\n}}",
  "properties": [
    {
      "name": "categories",
      "value": "[[Albums]]",
      "type": "multitext"
    },
    {
      "name": "artistUrl",
      "value": "https://open.spotify.com{{selector:[data-testid=\\\"creator-link\\\"]?href}}",
      "type": "multitext"
    },
    {
      "name": "artist",
      "value": "{{selector:[data-testid=\\\"creator-link\\\"]|wikilink}}",
      "type": "multitext"
    },
    {
      "name": "genre",
      "value": "",
      "type": "multitext"
    },
    {
      "name": "year",
      "value": "{{selector:[data-testid=\\\"release-date\\\"]}}",
      "type": "number"
    },
    {
      "name": "cover",
      "value": "{{selector:.bFtVZZnZgTWjjyzkPA5k img?src}}",
      "type": "text"
    },
    {
      "name": "url",
      "value": "{{url}}",
      "type": "multitext"
    },
    {
      "name": "rating",
      "value": "",
      "type": "number"
    },
    {
      "name": "tags",
      "value": "music",
      "type": "multitext"
    }
  ],
  "triggers": [
    "https://open.spotify.com/album/"
  ],
  "noteNameFormat": "{{selector:[data-testid=\"entityTitle\"]}}",
  "path": "References"
}
