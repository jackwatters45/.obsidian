filters:
  and:
    - note.categories.contains(link("Podcasts"))
    - '!file.name.contains("Template")'
    - '!file.inFolder("Categories")'
properties:
  file.name:
    displayName: Podcast
  note.host:
    displayName: Host
views:
  - type: table
    name: Table
    order:
      - file.name
      - host
