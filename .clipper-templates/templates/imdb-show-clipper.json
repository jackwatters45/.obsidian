{
	"schemaVersion": "0.1.0",
	"name": "IMDB Show",
	"behavior": "create",
	"noteContentFormat": "",
	"properties": [
		{
			"name": "categories",
			"value": "[[Shows]]",
			"type": "multitext"
		},
		{
			"name": "genre",
			"value": "{{schema:genre|wikilink|join}}",
			"type": "multitext"
		},
		{
			"name": "creator",
			"value": "{{selector:[data-testid=\"title-pc-principal-credit\"]:first-child a|unique|wikilink|join}}",
			"type": "multitext"
		},
		{
			"name": "rating",
			"value": "",
			"type": "number"
		},
		{
			"name": "scoreImdb",
			"value": "{{schema:aggregateRating.ratingValue}}",
			"type": "number"
		},
		{
			"name": "cast",
			"value": "{{selector:[data-testid=\"title-pc-principal-credit\"]:has(a[aria-label*=\"cast\"]) .ipc-metadata-list-item__list-content-item--link|unique|wikilink|join}}",
			"type": "multitext"
		},
		{
			"name": "cover",
			"value": "{{schema:image}}",
			"type": "text"
		},
		{
			"name": "imdbUrl",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "plot",
			"value": "{{schema:description}}",
			"type": "text"
		},
		{
			"name": "year",
			"value": "{{schema:datePublished|split:\"-\"|slice:0,1}}",
			"type": "number"
		},
		{
			"name": "created",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "last",
			"value": "",
			"type": "date"
		},
		{
			"name": "tags",
			"value": "to-watch, media",
			"type": "multitext"
		}
	],
	"triggers": [
		"schema:@TVSeries"
	],
	"noteNameFormat": "{{schema:name}}",
	"path": "References"
}
