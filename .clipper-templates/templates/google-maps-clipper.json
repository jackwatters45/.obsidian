{
  "schemaVersion": "0.1.0",
  "name": "Google Maps",
  "behavior": "create",
  "noteContentFormat": "",
  "properties": [
    {
      "name": "categories",
      "value": "[[Places]]",
      "type": "multitext"
    },
    {
      "name": "type",
      "value": "{{selector:button[jsaction*=\\\\\"category\\\\\"]|wikilink|join}}",
      "type": "multitext"
    },
    {
      "name": "mapsUrl",
      "value": "{{url}}",
      "type": "text"
    },
    {
      "name": "coordinates",
      "value": "{{url|split:\\\"/@\\\"|slice:1|split:\\\",\\\"|slice:0,2}}",
      "type": "multitext"
    },
    {
      "name": "address",
      "value": "{{selector:button[data-item-id=\\\\\"address\\\\\"] .Io6YTe}}",
      "type": "text"
    },
    {
      "name": "url",
      "value": "{{selector:a[data-item-id=\\\\\"authority\\\\\"]?href}}",
      "type": "multitext"
    },
    {
      "name": "rating",
      "value": "",
      "type": "number"
    },
    {
      "name": "scoreGoogle",
      "value": "{{selector:div[jsaction*=\\\"moreReviews\\\"] .fontDisplayLarge}}",
      "type": "number"
    },
    {
      "name": "description",
      "value": "",
      "type": "text"
    },
    {
      "name": "last",
      "value": "",
      "type": "date"
    },
    {
      "name": "tags",
      "value": "to-visit",
      "type": "multitext"
    }
  ],
  "triggers": [
    "https://www.google.com/maps/place/"
  ],
  "noteNameFormat": "{{selector:h1}}",
  "path": "References"
}
