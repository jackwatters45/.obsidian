filters:
  and:
    - categories.contains(link("Companies"))
    - '!file.name.contains("Template")'
    - '!file.inFolder("Categories")'
properties:
  file.name:
    displayName: Company
  property.url:
    displayName: Link
  note.url:
    displayName: URL
  note.people:
    displayName: People
views:
  - type: table
    name: Companies
    order:
      - file.name
      - url
      - careers
      - people
    columnSize:
      note.url: 239
