filters:
  and:
    - '!file.name.contains("Template")'
    - categories.contains(link("Projects"))
    - file.name != "Projects"
properties:
  note.status:
    displayName: Status
  note.url:
    displayName: URL
  file.name:
    displayName: Name
  note.type:
    displayName: Type
  note.year:
    displayName: Year
views:
  - type: table
    name: Table
    order:
      - file.name
      - type
      - year
      - status
      - url
      - github
    sort:
      - property: status
        direction: ASC
      - property: year
        direction: DESC
    columnSize:
      file.name: 209
      note.type: 199
